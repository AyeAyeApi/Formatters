{
    "name": "ayeaye/formatters",
    "type": "library",
    "description": "Serialise PHP objects into other types of object (json, xml, etc)",
    "homepage": "http://ayeayeapi.com",
    "minimum-stability": "stable",
    "license" : "MIT",
    "authors": [
        {
            "name": "Daniel Mason",
            "email": "daniel@ayeayeapi.com",
            "homepage": "http://danielmason.com",
            "role": "Lead developer"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "phploc/phploc": "^2.1",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.3",
        "sebastian/phpcpd": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AyeAye\\Formatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AyeAye\\Formatter\\Tests\\": "tests/"
        }
    },
    "config": {
        "github-protocols": ["https"]
    },
    "scripts" : {
        "test" : [
            "vendor/bin/phpunit"
        ]
    }
}
